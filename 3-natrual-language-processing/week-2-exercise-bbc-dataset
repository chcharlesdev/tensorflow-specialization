        "with open(\"/tmp/bbc-text.csv\", 'r') as csvfile:\n",
        "    data = csvfile.read()\n",
        "    lines = input.split(\"\\n\")\n",
        "    \n",
        "    for line in lines[1:]:\n",
        "      if(len(line) == 0):\n",
        "        continue\n",
        "      \n",
        "      split = line.split(',')\n",
        "      labels.append(split[0])\n",
        "\n",
        "      sentence = split[1]\n",
        "      sentence_split = sentence.split()\n",
        "      result = [ word for word in sentence_split if word.lower() not in stopwords]\n",
        "      sentence = ' '.join(result)\n",
        "      sentences.append(sentence)\n",
        "    \n",
        "print(len(labels))\n",
        "print(len(sentences))\n",
        "print(labels[0])\n",
        "print(sentences[0])\n",
        "# Expected Output\n",
